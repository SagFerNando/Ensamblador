.MODEL small
.CODE
INICIO:
	MOV AX, @DATA
	MOV DS,AX
	MOV DX,OFFSET TEXTO
	MOV AH,9
	INT 21H
	MOV DX,OFFSET TEXTO1
	MOV AH,9
	INT 21H
	MOV DX,OFFSET TEXTO2
	MOV AH,9
	INT 21H
	MOV DX,OFFSET TEXTO3
	MOV AH,9
	INT 21H
	MOV AX,4C0H
	INT 21H
.DATA
 TEXTO DB'HOLA MUNDO',13,10,'$'
 TEXTO1 DB'Este es un codigo ensamblador',13,10,'$'
 TEXTO2 DB'Es dificil cierto?',13,10,'$'
 TEXTO3 DB'Te gusta este lenguaje',13,10,'$'
.STACK
 DB 64 DUP ('PILA')
END INICIO
